{
  "swagger": "2.0",
  "info": {
    "description": "This is a contract for customer services, contains pending supply, estimations, portfolio management and reactivations",
    "version": "1.0.0",
    "title": "Services for data customers",
    "contact": {
      "email": "miasanchezm@elektra.com.mx"
    }
  },
  "host": "localhost:8080",
  "definitions": {
    "CustomerData": {
      "type": "object",
      "properties": {
        "uniquecustomerNumber": {
          "type": "string",
          "description": "cliente unico, pais-canal-sucursal-folio"
        },
        "name": {
          "type": "string",
          "description": "nombre del cliente"
        },
        "firstLastName": {
          "type": "string",
          "description": "apellido paterno del cliente"
        },
        "secondLastName": {
          "type": "string",
          "description": "apellido materno del cliente"
        },
        "email": {
          "type": "string",
          "description": "correo del cliente"
        },
        "phones": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Phones"
          }
        },
        "address": {
          "type": "string",
          "description": "direccion del cliente"
        },
        "totalPaymentCapacity": {
          "type": "number",
          "format": "double",
          "description": "capacidad de pago total"
        },
        "paymentCapacityAvailable": {
          "type": "number",
          "format": "double",
          "description": "capacidad de pago disponible"
        },
        "credlinestatus": {
          "type": "integer",
          "format": "int32",
          "description": "estatus de tabla cred linea de credito"
        },
        "cenlinestatus": {
          "type": "integer",
          "format": "int32",
          "description": "estatus de tabla cen linea de credito"
        },
        "descriptionCreditLine": {
          "type": "string",
          "description": "descripci√≥n del estatus de la linea de credito"
        },
        "photo": {
          "type": "string",
          "description": "url de la foto del cliente"
        },
        "gender": {
          "type": "string",
          "description": "sexo del cliente"
        },
        "UC": {
          "type": "string",
          "description": "undefined"
        },
        "overdraw": {
          "type": "number",
          "description": "undefined"
        },
        "idStatGest": {
          "type": "integer",
          "description": "status de gestion"
        },
        "idResGest": {
          "type": "integer",
          "description": "id del resultado de gestion"
        },
        "idDescGest": {
          "type": "string",
          "description": "descripcion de gestion"
        },
        "idFecGest": {
          "type": "string",
          "format": "date-time",
          "description": "fecha de gestion"
        }
      }
    },
    "Customers": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Customer"
      }
    },
    "Estimation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "numero de presupuesto"
        },
        "amount": {
          "type": "number",
          "format": "double",
          "description": "monto del presupuesto"
        },
        "calculationDate": {
          "type": "string",
          "format": "date-time"
        },
        "numberTerm": {
          "type": "integer",
          "format": "int64"
        },
        "descriptionTerm": {
          "type": "string"
        },
        "payment": {
          "type": "number",
          "format": "double"
        },
        "products": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Product"
          }
        }
      }
    },
    "Customer": {
      "type": "object",
      "properties": {
        "customerData": {
          "$ref": "#/definitions/CustomerData"
        },
        "estimations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Estimation"
          }
        }
      }
    },
    "Product": {
      "type": "object",
      "properties": {
        "SKUNumber": {
          "type": "integer",
          "format": "int64"
        },
        "SKUDescription": {
          "type": "string"
        },
        "quantity": {
          "type": "integer",
          "format": "int32"
        },
        "SKULineId": {
          "type": "integer",
          "format": "int64"
        },
        "SKULineDescription": {
          "type": "string"
        },
        "price": {
          "type": "number",
          "format": "double",
          "description": "precio del producto"
        }
      }
    },
    "Phones": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "phoneNumber": {
          "type": "string"
        }
      }
    },
    "ErrorType": {
      "type": "object",
      "properties": {
        "error_code": {
          "type": "integer",
          "format": "int64"
        },
        "message": {
          "type": "string"
        },
        "transactionID": {
          "type": "string"
        }
      }
    },
    "Error": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ErrorType"
      }
    }
  }
}